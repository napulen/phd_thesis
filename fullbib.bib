	pages = {219--249},
	file = {Brown - 1988 - The Interplay of Set Content and Temporal Context .pdf:/home/napulen/Zotero/storage/T3U8NKM5/Brown - 1988 - The Interplay of Set Content and Temporal Context .pdf:application/pdf;Snapshot:/home/napulen/Zotero/storage/X3H7INYV/219.html:text/html},
